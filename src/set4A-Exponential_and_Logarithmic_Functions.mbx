            <cell><m>2^{-1} = \frac{1}{2}</m></cell>
        </row>
        <row bottom = "minor">
            <cell><m>0</m></cell>
            <cell><m>2^0 = 1</m></cell>
        </row>
        <row bottom = "minor">
