            </paragraphs>
            <figure xml:id="figure-graph-point-8-to-neg-x"> <!--set4A-Exponential_and_Logarithmic_Functions_06.png-->
              <image xml:id="graph-point-8-to-neg-x">
                <latex-image-code><![CDATA[\begin{tikzpicture} 
                  \draw[step=1cm,gray,very thin] (-5.9,-.9) grid (5.9,6.9);
                  \draw[line width=0.55mm,black,<->] (-5.9,0) -- (5.9,0) node[anchor=north west] {$x$};
                  \draw[line width=0.55mm,black,<->] (0,-.9) -- (0,6.9) node[anchor=south east] {$y$};
                  \foreach \x in {-5,0,5}
                      \draw[line width=0.55mm,black] (\x cm,3pt) -- (\x cm,-3pt) node[shift={(0.05,-.3)}] {$\x$};
                  \draw[line width=0.55mm,black] (3pt, 4 cm) -- (-3pt,4 cm) node[shift={(-.3,0.05)}] {$20$};
                  \draw[line width=0.55mm,black] (3pt, 6 cm) -- (-3pt,6 cm) node[shift={(-.3,0.05)}] {$30$};
                  \draw[line width=0.75mm, domain=-1.46:5.9,smooth,variable=\x,red] plot ({\x},{5*.8^\x});
                  \fill[black] (-1,6.25) circle (.15cm);
                  \fill[black] (0,5) circle (.15cm);
                  \fill[black] (1,4) circle (.15cm);
