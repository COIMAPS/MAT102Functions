          \draw[line width=0.55mm,black] (3pt,3 cm) -- (-3pt,3 cm) node[shift={(-.3,0.05)}] {$12$};
          \draw[line width=0.55mm,black] (3pt,5 cm) -- (-3pt,5 cm) node[shift={(-.3,0.05)}] {$18$};
          \draw[line width=0.55mm,red] (0,3)--(1,5);
          \draw[line width=0.55mm,red] (1,5)--(1,3);
          \draw[line width=0.55mm,red] (1,3)--(2,5);
          \draw[line width=0.55mm,red] (2,5)--(2,3);
          \draw[line width=0.55mm,red] (2,3)--(3,5);
          \draw[line width=0.55mm,red] (3,5)--(3,3);
          \draw[line width=0.55mm,red] (3,3)--(4,5);
